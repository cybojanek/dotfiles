regexp=\`[A-Za-z0-9_():&*]+( const)?\'
colours=magenta
.........
# machine
regexp=\s(\-msoft\-float|\-mno\-sse|\-mtune=amdfam10)
colours=bold blue
.........
# various flags
regexp=\s(-std=c99|\-c|\-fPIC|\-fno\-builtin|\-O\d|\-r|\-g\d|\-g\b)
colours=green
.........
# output
regexp=\-o\s.+\b
colours=yellow
.........
# link libraries
regexp=\s\-l[A-Za-z0-9]+\s
colours=magenta
.........
# include dirs
regexp=\s\-I[\./A-Za-z0-9]+\s
colours=magenta
.........
# include dirs
regexp=\s(\-nostdinc|\-nostdlib|\-shared|\-soname|\-\-dynamic\-linker|\-\-rpath\-link)[=a-zA-Z\./]*
colours=magenta
